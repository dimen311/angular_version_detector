{
    "manifest_version": 3,
    "browser_specific_settings": {
        "gecko": {
            "id": "angular-detector@dimen311"
        }
    },
    "name": "Angular Version Detector",
    "version": "1.0.1",
    "homepage_url": "https://github.com/username/angular-detector",
    "author": "dimen311",
    "description": "Detects Angular framework and displays version information",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "styles.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}